      <TextArea fx:id="textArea" layoutX="25.0" layoutY="25.0" prefHeight="350.0" prefWidth="470.0" />
      <Button fx:id="report1" layoutX="525.0" layoutY="25.0" mnemonicParsing="false" onAction="#runReportOneHandle" text="Report 1" />
      <Button fx:id="report2" layoutX="525.0" layoutY="75.0" mnemonicParsing="false" onAction="#runReportTwoHandle" text="Report 2" />
      <Button fx:id="report3" layoutX="525.0" layoutY="125.0" mnemonicParsing="false" onAction="#runReportThreeHandle" text="Report 3" />
   </children>
</AnchorPane>
